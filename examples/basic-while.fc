#include <cstdlib.fh>

int main(int argc, char[][] argv) {
    // Sum all numbers from 1 to 10 (included) using a while loop
    
    int i = 0;
    int sum = 0;
    puts("Before loop\n");
    while (i <= 10) {
    	printf("Iteration %d\n", i);
    	sum += i;
    	i += 1;
    }
    puts("After loop\n");

	printf("Sum = %d\n", sum);
    
    return 0;
}
