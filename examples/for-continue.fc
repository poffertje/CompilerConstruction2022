#include <cstdlib.fh>

int main(int argc, char[][] argv) {
    // Sum all numbers from 1 to 10 (included) using a for loop
    
    int sum = 0;
    puts("Before loop\n");
    for (int i = 0 to 11) {
    	if (i == 3) {          
		continue;
	}
    	printf("Iteration %d\n", i);
    	sum += i;
    }
    puts("After loop\n");

	printf("Sum = %d\n", sum);
    
    return 0;
}
